<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:sap.ui.layout.form="sap.ui.layout.form">
	<Dialog title="Terms and conditions" resizable="true" contentWidth="auto" draggable="true">
		<content>
			<sap.ui.layout.form:SimpleForm id="termForms" editable="true" layout="ResponsiveGridLayout">
				<sap.ui.layout.form:content>
					<FormattedText htmlText="{/HTML}"/>
					<!--<Text text="{i18n>SSEGTerms}"/>-->
				</sap.ui.layout.form:content>
			</sap.ui.layout.form:SimpleForm>
			<Toolbar>
				<ToolbarSpacer/>
				<Button text="Accept" type="Accept" width="100px" press="onNewAppPress"/>
				
				<Button text="Reject" type="Reject"  width="100px" press="onCancelTerms"/>
			</Toolbar>
		</content>
		<beginButton></beginButton>
		<endButton></endButton>
	</Dialog>
</core:FragmentDefinition>